{
  "builds": [
    { "src": "frontend/index.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/frontend/index.html" },
    { "src": "/frontend/(.*)", "dest": "/frontend/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
